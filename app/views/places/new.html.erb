<%=form_with model: @place do |form|%>
<%= form.select :country, collection: CountryStateSelect.countries_collection %>
<div>
<%=form.text_field :place_name,placeholder:"place name"%>
</div>
<div>
<%=form.text_area :place_description,placeholder:"place description"%>
</div>
<%=form.submit "submit",class:"btn btn-success" %>
<%end%>